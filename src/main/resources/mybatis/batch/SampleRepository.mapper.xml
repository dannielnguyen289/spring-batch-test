<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.stringeex.sample.module.sample.SampleRepository">

    <insert id="insert"
            parameterType="com.stringeex.sample.module.sample.query.InsertEntityPrt"
            useGeneratedKeys="true" keyProperty="id" keyColumn="ID">
        INSERT INTO SAM_ENTITY (
            NAME,
            DESCRIPTION
        ) VALUES (
                     #{name},
                     #{description}
                 )
    </insert>

</mapper>